Procedure set_3d_dot_gab is
   x0, x1, x2, x3 : Real;
begin
   x0 := 2.0*N;
   x1 := -Kxy*x0;
   x2 := -Kxz*x0;
   x3 := -Kyz*x0;
   dot_gab (xx) := -Kxx*x0;
   dot_gab (xy) := x1;
   dot_gab (xz) := x2;
   dot_gab (xy) := x1;
   dot_gab (yy) := -Kyy*x0;
   dot_gab (yz) := x3;
   dot_gab (xz) := x2;
   dot_gab (yz) := x3;
   dot_gab (zz) := -Kzz*x0;
end set_3d_dot_gab;

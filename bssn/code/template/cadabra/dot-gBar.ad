Procedure set_3d_dot_gBar is
   x0, x1, x2, x3 : Real;
begin
   x0 := 2.0*N;
   x1 := -ABar_xy*x0;
   x2 := -ABar_xz*x0;
   x3 := -ABar_yz*x0;
   dot_gBar (xx) := -ABar_xx*x0;
   dot_gBar (xy) := x1;
   dot_gBar (xz) := x2;
   dot_gBar (xy) := x1;
   dot_gBar (yy) := -ABar_yy*x0;
   dot_gBar (yz) := x3;
   dot_gBar (xz) := x2;
   dot_gBar (yz) := x3;
   dot_gBar (zz) := -ABar_zz*x0;
end set_3d_dot_gBar;
